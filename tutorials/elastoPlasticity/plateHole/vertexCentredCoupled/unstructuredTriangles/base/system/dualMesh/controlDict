FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     solids4Foam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         1;

deltaT          1;

writeControl    timeStep;

writeInterval   1;

cycleWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression uncompressed;

timeFormat      general;

timePrecision   6;

runTimeModifiable no;

functions
{
    analyticalSolution
    {
        type          plateHoleAnalyticalSolution;
        farFieldTractionX 1e6;
        holeRadius    0.5;
        E             200e+9;
        nu            0.3;
        cellDisplacement yes;
        pointDisplacement no;
        cellStress yes;
        pointStress no;
    }
}

