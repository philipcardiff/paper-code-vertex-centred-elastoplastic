FoamFile
{
    version     2.0;
    format      ascii;
    class       pointVectorField;
    object      pointD;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    right
    {
        type            uniformFixedValue;
        uniformValue    (0 0 0);
    }

    top
    {
        type            uniformFixedValue;
        uniformValue    (0 0 0);
    }

    bottom
    {
        type            uniformFixedValue;
        uniformValue    (0 0 0);
    }

    left
    {
        type            uniformFixedValue;
        uniformValue    (0 0 0);
    }

    back
    {
        type            uniformFixedValue;
        uniformValue    (0 0 0);
    }

    front
    {
        type            uniformFixedValue;
        uniformValue    (0 0 0);
    }
}

