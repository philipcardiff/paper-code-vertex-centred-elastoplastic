FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvOptions;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

bodyForce
{
    type            customBodyForce;
    active          true;

    // Force function coefficients
    ax          2;
    ay          4;
    az          6;
    mu          7.6923e10;
    lambda      1.1538e11;
}


